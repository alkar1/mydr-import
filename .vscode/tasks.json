{
  "tasks": [
    {
      "problemMatcher": "$msCompile",
      "label": "build",
      "args": [
        "build",
        "${workspaceFolder}/MyDr_Import.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "type": "process",
      "command": "dotnet",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "command": "dotnet",
      "problemMatcher": "$msCompile",
      "type": "process",
      "args": [
        "publish",
        "${workspaceFolder}/MyDr_Import.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "label": "publish"
    },
    {
      "command": "dotnet",
      "problemMatcher": "$msCompile",
      "type": "process",
      "args": [
        "clean",
        "${workspaceFolder}/MyDr_Import.csproj"
      ],
      "label": "clean"
    }
  ],
  "version": "2.0.0"
}
